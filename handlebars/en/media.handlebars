<!DOCTYPE html>
<html lang="en">
{{>head}}

<body>

    {{>header_white_en page="Bisnis"}}
    <!-- s:panel -->
    <div id="panel">
        <div class="parallax">
            <div class="parallax__slider">
                <div class="parallax__item">
                    <div class="jumbotron" style="background: url('asset/img/media/media-slide-1.jpg') no-repeat center; background-size: cover;" data-paroller-factor="0.4" data-paroller-factor-xs="0.2" data-paroller-type="background" data-paroller-direction="vertical"></div>
                </div>
                <div class="parallax__item">
                    <div class="jumbotron" style="background: url('asset/img/media/media-slide-2.jpg') no-repeat center; background-size: cover;" data-paroller-factor="0.3" data-paroller-factor-xs="0.2" data-paroller-type="background" data-paroller-direction="vertical"></div>
                </div>
                <div class="parallax__item">
                    <div class="jumbotron" style="background: url('asset/img/media/media-slide-3.jpg') no-repeat center; background-size: cover;" data-paroller-factor="0.1" data-paroller-factor-xs="0.2" data-paroller-type="background" data-paroller-direction="vertical"></div>
                </div>
            </div>
            <div class="parallax__text">
                <div class="panel__title panel__title--right mb50">
                    <h1 class="panel__title__text">Media</h1>
                </div>
                <div class="panel__paragraph panel__text--lstripe text-left mb150">
                    <div class="panel__text__wrapper">
                        <p class="panel__text panel__text--big">Complete and accurate information submitted by more than <b>150</b> brands of print, online, and broadcast media makes Kompas Gramedia the largest media company in Indonesia</p>
                    </div>
                </div>
            </div>
            <div class="parallax__down"><a class="parallax__btn"><span class="icon-chevron-down"></span></a></div>
        </div>
        <div class="media__bg" id="section-2">
            <div class="panel__ornament">
                <img src="asset/img/bg-ornament.png" alt="bg-ornament.png" class="panel__ornament--left">
                <img src="asset/img/bg-ornament.png" alt="bg-ornament.png" class="panel__ornament--right">
            </div>
            <div class="container clearfix business__detail">
                <div class="panel__wrapper col-30">
                    <!-- s:media filter-->
                    <div class="accordion__group wrap" id="accordion" role="tablist" aria-multiselectable="true">
                        <div class="accordion__hero">
                            <h2 class="accordion__hero--text">Media Classification</h2>
                        </div>
                        {{#each media_en}}
                        <div class="pnl">
                            <div class="accordion__heading{{#if status}} active{{else}}{{/if}}" role="tab" id="{{heading}}">
                                <h4 class="accordion__title">
                                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#{{collapse}}" aria-expanded="true" aria-controls="{{collapse}}">{{title}}</a>
                                </h4>
                            </div>
                            <div id="{{collapse}}" class="accordion__collapse collapse{{#if status}} show{{else}}{{/if}}" role="tabpanel" aria-labelledby="{{heading}}">
                                <div class="accordion__body">
                                    <ul class="accordion__filter checkbox">
                                        {{#each submenu}}
                                        <li>
                                            <input id="{{no}}-option" type="checkbox" class="checkbox__input" />
                                            <label for="{{no}}-option" class="checkbox__label">{{title}}</label>
                                        </li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        </div>
                        {{/each}}
