                    <div class="accordion__hero">
                        <h2 class="accordion__hero--text">Klasifikasi Media</h2>
                    </div>
                    {{#each media}}
                    <div class="pnl">
                        <div class="accordion__heading{{#if status}} active{{else}}{{/if}}" role="tab" id="{{heading}}">
                            <h4 class="accordion__title">
                                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#{{collapse}}"
                                    aria-expanded="true" aria-controls="{{collapse}}">{{title}}</a>
                            </h4>
                        </div>
                        <div id="{{collapse}}" class="accordion__collapse collapse{{#if status}} show{{else}}{{/if}}"
                            role="tabpanel" aria-labelledby="{{heading}}">
                            <div class="accordion__body">
                                <ul class="accordion__filter checkbox">
                                    {{#each submenu}}
                                    <li>
                                        <input id="{{no}}-option" type="checkbox" class="checkbox__input" />
                                        <label for="{{no}}-option" class="checkbox__label">{{title}}</label>
                                    </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
                <!-- e:media filter-->
